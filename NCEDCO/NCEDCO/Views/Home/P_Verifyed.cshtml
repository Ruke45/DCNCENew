@model NCEDCO.Models.M_Cerificate

@if(Model!= null){
<div class="row">
    <div class="alert alert-success">
        <strong>Verified !</strong> This Document has been Authenticated by the NCE.
    </div>
    
    <div style="width: 70%; margin: 0px auto;">
        <table class="table table-striped table-bordered">
            <tr>
                <td>Consignor Name</td>
                <td>@Model.Consignor_Exporter</td>
            </tr>
            <tr>
                <td>Certificate(CO) Issued Date</td>
                <td>@Model.InvoiceDate</td>
            </tr>
            <tr>
                <td>Consignee</td>
                <td>@Model.Consignee</td>
            </tr>
            <tr>
                <td>Invoice Number</td>
                <td>@Model.InvoiceNo</td>
            </tr>
        </table>
    </div>
</div>
}
else{
<div class="row">
    <div class="alert alert-warning">
        <strong>Error !</strong> This Document has NOT been Authenticated by the National Chamber of Exporters of Sri Lanka (NCE).
    </div>
</div>
}
