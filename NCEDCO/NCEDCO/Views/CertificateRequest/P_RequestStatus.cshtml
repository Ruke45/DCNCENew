@model IEnumerable<NCEDCO.Models.M_Cerificate>

<div class="panel panel-default">
            <div class="panel-heading">Certificate Request Status | <label>From : @ViewBag.Fdate</label> - <label>To : @ViewBag.Tdate </label> </div>
            <div class="panel-body">
                <table id="datatable" class="table table-striped table-bordered" style="font-size: 12px;">
                    <thead>
                        <tr>
                            <th>Request Type</th>
                            <th>Request Id</th>
                            <th>@Html.DisplayNameFor(model => model.InvoiceNo)</th>
                            <th>@Html.DisplayNameFor(model => model.Status)</th>
                            <th>@Html.DisplayNameFor(model => model.RequestDate)</th>
                            <th>Client</th>
                        </tr>
                    </thead>


                    <tbody>
                        @if (Model != null)
                        {
                            foreach (var item in Model)
                            {

                            <tr>
                                <td>@Html.DisplayFor(modelItem => item.Ctype)</td>
                                <td>@Html.DisplayFor(modelItem => item.RequestReff)</td>
                                <td>@Html.DisplayFor(modelItem => item.InvoiceNo)</td>
                                <td>@Html.DisplayFor(modelItem => item.Status)</td>
                                <td>@Html.DisplayFor(modelItem => item.RequestDate)</td>
                                <td><a data-toggle="tooltip" title="Parent : @item.ParentId">@item.Client_Id</a></td>
                            </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
            <div class="panel-footer">Certificate Request Status</div>
        </div>

<script src="~/Scripts/pace/pace.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('#datatable').dataTable({ "order": [[3, "desc"]] });
        $('#datatable-keytable').DataTable({
            keys: true
        });
        $('#datatable-responsive').DataTable();
        $('#datatable-scroller').DataTable({
            ajax: "js/datatables/json/scroller-demo.json",
            deferRender: true,
            scrollY: 380,
            scrollCollapse: true,
            scroller: true
        });
        var table = $('#datatable-fixed-header').DataTable({
            fixedHeader: true
        });
    });
    TableManageButtons.init();

</script>