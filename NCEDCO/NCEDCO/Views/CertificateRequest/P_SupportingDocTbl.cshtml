@model IEnumerable<NCEDCO.Models.M_SupportDocumentUpload>


    <div class="panel-heading">Pending Certificate Requests Supporting Docs</div>
 
            <div class="panel-body">
                @if (Model != null)
                {
                    if(Model.Count() > 0){
                    <table id="datatable" class="table table-striped table-bordered" style="font-size: 12px;">
                        <thead>
                            <tr>
                                <th>@Html.DisplayNameFor(model => model.RequestRefNo)</th>
                                <th>@Html.DisplayNameFor(model => model.DocumentTitle)</th>
                                <th>@Html.DisplayNameFor(model => model.SignatureRequired)</th>
                                @*<th>@Html.DisplayNameFor(model => model.SupportingDocumentID)</th>*@
                                <th>Action</th>
                            </tr>
                        </thead>


                        <tbody>

                            @foreach (var item in Model)
                            {

                                <tr>
                                    <td>@Html.DisplayFor(modelItem => item.RequestRefNo)</td>
                                    <td>@Html.DisplayFor(modelItem => item.DocumentTitle)</td>
                                    <td>@Html.DisplayFor(modelItem => item.SignatureRequired)</td>
                                    @*<td>@Html.DisplayFor(modelItem => item.SupportingDocumentID)</td>*@
                                    <td align="center"><a href="#" class="btn btn-primary btn-sm"  onclick='viewSupporting("@item.UploadedPath")'>View</a></td>

                                </tr>
                            }

                        </tbody>
                    </table>
                  }
                  else{
                        <label>No Supporting Documents Available</label>
                    }
                }
            </div>
